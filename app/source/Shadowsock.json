        {
            "type": "title",
            "value": "操作"
        },
        {
            "type": "action",
            "value": "安装或重装或更新 Shadowsock",
            "action": "cd /tmp && wget https://raw.githubusercontent.com/hq450/fancyss/master/fancyss_arm/shadowsocks.tar.gz && tar -zxvf /tmp/shadowsocks.tar.gz && chmod +x /tmp/shadowsocks/install.sh && sh /tmp/shadowsocks/install.sh"
        },
        {
            "type": "action",
            "value": "重启 Shadowsock 服务",
            "action": "sh /koolshare/scripts/ss_config.sh"
        },
        {
            "type": "action",
            "value": "服务器订阅更新",
            "action": "sh /koolshare/scripts/ss_online_update.sh"
        },
        {
            "type": "action",
            "value": "重新拨号",
            "action": "service restart_wan"
        }
    ]
