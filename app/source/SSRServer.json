{
  "version": "2",
  "name": "ShadowsockR 服务器",
  "category": "server",
  "data": [
    {
      "type": "webview",
      "value": "https://shadowsocks.be/9.html"
    },
    {
      "type": "title",
      "value": "信息"
    },
    {
      "type": "message",
      "value": "脚本要求",
      "action": "需要 root 用户执行"
    },
    {
      "type": "title",
      "value": "Ubuntu/Debian 使用"
    },
    {
      "type": "action",
      "value": "ShadowsocksR 一键安装",
      "action": "wget --no-check-certificate https://raw.githubusercontent.com/teddysun/shadowsocks_install/master/shadowsocksR.sh && chmod +x shadowsocksR.sh && ./shadowsocksR.sh 2>&1 | tee shadowsocksR.log"
    },
    {
      "type": "title",
      "value": "操作"
    },
    {
      "type": "action",
      "value": "重启",
      "action": "/etc/init.d/shadowsocks restart && exit"
    },
    {
      "type": "action",
      "value": "状态",
      "action": "/etc/init.d/shadowsocks status"
    },
    {
      "type": "action",
      "value": "查看链接",
      "action": "cat /etc/shadowsocks.json"
    },
    {
      "type": "action",
      "value": "卸载 ",
      "action": "./shadowsocksR.sh uninstall"
    }
  ]
}