{
  "version": "2",
  "name": "bbr",
  "data": [
    {
      "type": "title",
      "value": "信息"
    },
    {
      "type": "message",
      "value": "脚本说明",
      "action": "sysctl net.ipv4.tcp_available_congestion_control"
    },
    {
      "type": "title",
      "value": "指令"
    },
    {
      "type": "action",
      "value": "更新软件源并安装 Git",
      "action": "apt-get update && apt-get -y install git"
    },
    {
      "type": "action",
      "value": "克隆脚本内容到服务器",
      "action": "git clone https://github.com/flyzy2005/ss-fly"
    },
    {
      "type": "action",
      "value": "交互式安裝 SSR",
      "action": "ss-fly/ss-fly.sh -ssr"
    },
    {
      "type": "action",
      "value": "重启 SS",
      "action": "/etc/init.d/shadowsocks restart"
    },
    {
      "type": "action",
      "value": "状态",
      "action": "/etc/init.d/shadowsocks status"
    },
    {
      "type": "action",
      "value": "查看 SS 链接",
      "action": "ss-fly/ss-fly.sh -sslink"
    }
  ]
}