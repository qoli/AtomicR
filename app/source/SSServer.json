    {
      "type": "action",
      "value": "shadowsocks-libev 一键安装",
      "action": "wget --no-check-certificate -O shadowsocks-libev-debian.sh https://raw.githubusercontent.com/teddysun/shadowsocks_install/master/shadowsocks-libev-debian.sh && chmod +x shadowsocks-libev-debian.sh && ./shadowsocks-libev-debian.sh 2>&1 | tee shadowsocks-libev-debian.log"
    },
    {
      "type": "title",
      "value": "操作"
    },
    {
      "type": "action",
      "value": "重启",
      "action": "/etc/init.d/shadowsocks restart"
    },
    {
      "type": "action",
      "value": "查看状态",
      "action": "/etc/init.d/shadowsocks status"
    },
    {
      "type": "action",
      "value": "卸载",
      "action": "./shadowsocks-libev-debian.sh uninstall"
    }
